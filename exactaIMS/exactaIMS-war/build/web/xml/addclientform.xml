<?xml version="1.0" encoding="UTF-8"?>
<root>
    <div id="addclient">
        <h2>Add Client</h2>
        <form action="addclient" method="post" id="addclientform">
            Client Name:<input type="text" name="name"><br/>
            Client CSS:<input type="text" name="css"><br/>
            Client Logo:<input type="text" name="logo"><br/>
            Client Email:<input type="text" name="email"><br/>
            Client Url:<input type="text" name="url"><br/>
            Client Phone Number:<input type="text" name="phonenumber"><br/>
            <input type="submit" value="Add">
        </form>
    </div>
</root>
